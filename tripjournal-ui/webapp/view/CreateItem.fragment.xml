<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
  <Dialog id="_IDGenDialog" title="Trip Item">
    <content>
      <f:SimpleForm id="_IDGenSimpleForm2">
        <Label id="_IDGenLabel9" text="Date"/>
        <DatePicker id="_IDGenDatePicker" dateValue="{createItem>/Ddate}" displayFormat="yyyy.MM.dd"/>

        <Label id="_IDGenLabel10" text="From"/>
        <Input id="_IDGenInput8" value="{createItem>/FromAddr}"/>

        <Label id="_IDGenLabel11" text="To"/>
        <Input id="_IDGenInput9" value="{createItem>/ToAddr}"/>

        <Label id="_IDGenLabel12" text="Distance (km)"/>
        <Input id="_IDGenInput10" value="{createItem>/Distance}"/>

        <Label id="_IDGenLabel13" text="Note"/>
        <TextArea id="_IDGenTextArea" value="{createItem>/Note}"/>
      </f:SimpleForm>
    </content>

    <beginButton>
      <Button id="_IDGenButton8" text="Save" press=".onCreateItem"/>
    </beginButton>
    <endButton>
      <Button id="_IDGenButton9" text="Cancel" press=".onCancelItemDialog"/>
    </endButton>
  </Dialog>
</core:FragmentDefinition>
