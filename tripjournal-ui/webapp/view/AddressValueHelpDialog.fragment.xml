<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
  <ValueHelpDialog id="addrVHD"
        title="{i18n>vh.addr.title}"
        supportRanges="false"
        supportMultiselect="false"
        key="Id"                      
        ok=".onAddrVhOk"      
        cancel=".onAddrVhCancel"
        afterClose=".onAddrVhAfterClose">

    <filterBar>
      <fb:FilterBar id="_IDGenFilterBar1" search=".onAddrFilterBarSearch" isRunningInValueHelpDialog="true">
        <fb:filterGroupItems>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem4" groupName="__$INTERNAL$" name="Name"     label="{i18n>vh.addr.name}">
            <fb:control><m:Input id="_IDGenInput8" name="Name"/></fb:control>
          </fb:FilterGroupItem>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem5" groupName="__$INTERNAL$" name="City"     label="{i18n>vh.addr.city}">
            <fb:control><m:Input id="_IDGenInput9" name="City"/></fb:control>
          </fb:FilterGroupItem>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem6" groupName="__$INTERNAL$" name="PostCode" label="{i18n>vh.addr.postcode}">
            <fb:control><m:Input id="_IDGenInput11" name="PostCode"/></fb:control>
          </fb:FilterGroupItem>
        </fb:filterGroupItems>
      </fb:FilterBar>
    </filterBar>
  </ValueHelpDialog>
</core:FragmentDefinition>