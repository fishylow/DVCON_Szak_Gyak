<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
  <ValueHelpDialog id="carVHD"
        title="{i18n>vh.car.title}"
        supportRanges="false"
        supportMultiselect="false"
        key="LicensePlate"
        descriptionKey="Type"
        ok=".onCarVhOk"
        cancel=".onCarVhCancel"
        afterClose=".onCarVhAfterClose">
    <filterBar>
      <fb:FilterBar id="_IDGenFilterBar" search=".onCarFilterBarSearch" isRunningInValueHelpDialog="true">
        <fb:filterGroupItems>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem1" groupName="__$INTERNAL$" name="LicensePlate" label="{i18n>vh.car.plate}">
            <fb:control><m:Input id="_IDGenInput2" name="LicensePlate"/></fb:control>
          </fb:FilterGroupItem>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem2" groupName="__$INTERNAL$" name="Manufacturer" label="{i18n>vh.car.make}">
            <fb:control><m:Input id="_IDGenInput6" name="Manufacturer"/></fb:control>
          </fb:FilterGroupItem>
          <fb:FilterGroupItem id="_IDGenFilterGroupItem3" groupName="__$INTERNAL$" name="Type" label="{i18n>vh.car.model}">
            <fb:control><m:Input id="_IDGenInput14" name="Type"/></fb:control>
          </fb:FilterGroupItem>
        </fb:filterGroupItems>
      </fb:FilterBar>
    </filterBar>
  </ValueHelpDialog>
</core:FragmentDefinition>